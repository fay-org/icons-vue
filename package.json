{
  "name": "@fect-ui/vue-icons",
  "version": "0.1.6",
  "public": true,
  "author": "Fect Org",
  "license": "MIT",
  "description": "Well prepared high quality Icon Library for fect",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "LISCENSE",
    "dist/",
    "CHANGELOG.md",
    "README.md"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "update:source": "node ./scripts/updateSource.js",
    "generator": "ts-node ./scripts/generator.ts && ts-node ./scripts/collect.ts",
    "clean": "rm -rf ./dist",
    "build:types": "tsc -p ./scripts/tsconfig.json",
    "build:mode": "ts-node ./scripts/build.ts",
    "build:lib": "npm run clean &&npm run generator && npm run build:types && npm run build:mode",
    "release": "npm publish --access public --non-interactive"
  },
  "peerDependencies": {
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/core": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@fect-ui/themes": "^1.0.7",
    "@types/babel__core": "^7.1.14",
    "@types/fs-extra": "^9.0.11",
    "@types/jsdom": "^16.2.11",
    "@types/node": "^15.12.2",
    "@types/svgo": "^2.3.0",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/babel-plugin-jsx": "^1.0.6",
    "@vue/compiler-sfc": "^3.0.5",
    "axios": "^0.21.1",
    "fs-extra": "^10.0.0",
    "jsdom": "^16.6.0",
    "svgo": "^2.3.0",
    "ts-node": "^10.0.0",
    "tslib": "^2.2.0",
    "typescript": "^4.3.2",
    "vite": "^2.3.7",
    "vue": "^3.0.5",
    "vue-tsc": "^0.0.24"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fay-org/icons-vue"
  },
  "homepage": "https://github.com/fay-org/icons-vue",
  "keywords": [
    "vue component",
    "fect",
    "fect icon",
    "vue ui",
    "quality SVG"
  ]
}